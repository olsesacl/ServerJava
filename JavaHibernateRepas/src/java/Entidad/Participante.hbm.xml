<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Entidad.Participante" table="participantes">
        <id column="Dorsal" name="dorsal" type="integer"></id>
        <property column="Nombre" name="nombre"></property>
        <property column="Apellidos" name="apellidos"></property>
        <property column="Poblacion" name="poblacion"></property>
        <property column="CLUB" name="club"></property>
    </class>  
</hibernate-mapping>
