<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ParticipanteController</servlet-name>
        <servlet-class>Controller.ParticipanteController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarParticipante</servlet-name>
        <servlet-class>Controller.RegistrarParticipante</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarParticipante</servlet-name>
        <servlet-class>Controller.EditarParticipante</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BorrarParticipante</servlet-name>
        <servlet-class>Controller.BorrarParticipante</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ParticipanteController</servlet-name>
        <url-pattern>/ParticipanteController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarParticipante</servlet-name>
        <url-pattern>/RegistrarParticipante</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarParticipante</servlet-name>
        <url-pattern>/EditarParticipante</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BorrarParticipante</servlet-name>
        <url-pattern>/BorrarParticipante</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>ParticipanteController</welcome-file>
    </welcome-file-list>
</web-app>
